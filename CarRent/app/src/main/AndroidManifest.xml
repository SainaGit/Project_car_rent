<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="sdeveloper.car.rent" >

    <application
        android:name=".application.CarRent"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".splashscreen.ActivitySplashScreen"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".tutorial.ActivityTutorial"
            android:label="@string/title_activity_activity_tutorial" >
        </activity>
        <activity
            android:name=".login.ActivityLogin"
            android:label="@string/title_activity_acitivity_login" >
        </activity>
        <activity
            android:name=".register.ActivityRegister"
            android:label="@string/title_activity_activity_register" >
        </activity>
        <activity
            android:name=".main.MainActivity"
            android:label="@string/title_activity_main" >
        </activity>
    </application>

</manifest>
